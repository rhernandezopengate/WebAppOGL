@model WebAppOGL.Entities.OrdenesCompra.oc_supervisores

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_LayoutOrdenesCompras.cshtml";
}
<div class="form-horizontal">
    <h2>Editar Supervisor</h2>
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    @Html.HiddenFor(model => model.Id)

    <div class="form-group">
        <label class="control-label col-md-2">Nombre Completo</label>
        <div class="col-md-10">
            @Html.EditorFor(model => model.NombreCompleto, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.NombreCompleto, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        <label class="control-label col-md-2">Empleados</label>
        <div class="col-md-10">
            @Html.EditorFor(model => model.adm_empleados_Id, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.adm_empleados_Id, "", new { @class = "text-danger" })
        </div>
    </div>

   

</div>


<div class="ln_solid"></div>
<div class="form-actions no-color">
    <a class="btn btn-default" href="@Url.Action("Index")">Regresar</a>|
    <input type="submit" value="Save" class="btn btn-primary" />
</div>


