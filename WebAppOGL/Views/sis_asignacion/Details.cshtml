                                                        <input type="text" class="form-control" placeholder="Amount" value="@Model.sis_equipos.Cargador">
                                                        <div class="input-group-addon"><span class="fa fa-calendar"></span> </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </center>
                                </div>
                            </div>
                        </div>
                        <div class="panel">
