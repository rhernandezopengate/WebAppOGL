@model WebAppOGL.Entities.Sistemas.sis_asignacion

@{
    ViewBag.Title = "Details";
    Layout = null; //"~/Views/Shared/_LayoutSistemas.cshtml";
}

<div class="modal-content">

    <div class="modal-body">
        <div class="col-md-12 col-sm-12">
