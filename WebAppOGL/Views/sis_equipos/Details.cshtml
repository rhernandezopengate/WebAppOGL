@model WebAppOGL.Entities.Sistemas.sis_equipos

@{
    ViewBag.Title = "Details";
    Layout = null;
}

<div class="modal-content">

    <div class="modal-body">
        <div class="col-md-12 col-sm-12">
            <div class="x_panel">
                <div class="x_title">
                    <center>
                        <h3><i class="fa fa-desktop"></i> Detalle del Equipo <br /> @Model.Nombre_Equipo</h3>
                    </center>
                    <div class="clearfix"></div>
                </div>
                <div class="x_content">
                    <div class="accordion" id="accordion" role="tablist" aria-multiselectable="true">
                        <div class="panel">
                            <a class="panel-heading" role="tab" id="headingOne" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                <h4 class="panel-title">Informacion del Equipo</h4>
                            </a>
                            <div id="collapseOne" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
                                <div class="panel-body">
                                    <center>
                                        <div class="form-inline">
                                            <div class="form-group">
                                                <div class="input-group">
                                                    <div class="input-group-addon">Fecha Alta</div>
                                                    <input type="text" class="form-control" placeholder="Amount" value="@Model.Fecha_Alta">
                                                    <div class="input-group-addon"><span class="fa fa-calendar"></span> </div>
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <div class="input-group">
                                                    <div class="input-group-addon">Fecha Compra</div>
                                                    <input type="text" class="form-control" placeholder="Amount" value="@Model.Fecha_Alta">
                                                    <div class="input-group-addon"><span class="fa fa-calendar"></span> </div>
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <div class="input-group">
                                                    <div class="input-group-addon">Modelo &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</div>
                                                    <input type="text" class="form-control" placeholder="Amount" value="@Model.Modelo">
                                                    <div class="input-group-addon"><span class="fa fa-book"></span> </div>
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <div class="input-group">
                                                    <div class="input-group-addon">Numero Serie</div>
                                                    <input type="text" class="form-control" placeholder="Amount" value="@Model.Numero_Serie">
                                                    <div class="input-group-addon"><span class="fa fa-qrcode"></span> </div>
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <div class="input-group">
                                                    <div class="input-group-addon">Numero Parte</div>
                                                    <input type="text" class="form-control" placeholder="Amount" value="@Model.Numero_Parte">
                                                    <div class="input-group-addon"><span class="fa fa-asterisk"></span> </div>
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <div class="input-group">
                                                    <div class="input-group-addon">Color &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</div>
                                                    <input type="text" class="form-control" placeholder="Amount" value="@Model.Color">
                                                    <div class="input-group-addon"><span class="fa fa-paint-brush"></span> </div>
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <div class="input-group">
                                                    <div class="input-group-addon">Cargador &nbsp;</div>
                                                    <input type="text" class="form-control" placeholder="Amount" value="@Model.Cargador">
                                                    <div class="input-group-addon"><span class="fa fa-plug"></span> </div>
                                                </div>
                                            </div>
                                        </div>
                                    </center>                       
                                </div>
                            </div>
                        </div>
                        <div class="panel">
                            <a class="panel-heading collapsed" role="tab" id="headingTwo" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                                <h4 class="panel-title">Informacion Fisica del Equipo</h4>
                            </a>
                            <div id="collapseTwo" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingTwo">
                                <div class="panel-body">
                                    <center>
                                        <div class="form-inline">
                                            <div class="form-group">

                                                <div class="input-group">
                                                    <div class="input-group-addon">Nombre Equipo</div>
                                                    <input type="text" class="form-control" placeholder="Amount" value="@Model.Nombre_Equipo">
                                                    <div class="input-group-addon"><span class="fa fa-bookmark"></span> </div>
                                                </div>

                                                <div class="input-group">
                                                    <div class="input-group-addon">MAC Ethernet</div>
                                                    <input type="text" class="form-control" placeholder="Amount" value="@Model.MAC_Ethernet">
                                                    <div class="input-group-addon"><span class="fa fa-tag"></span> </div>
                                                </div>

                                                <div class="input-group">
                                                    <div class="input-group-addon">MAC WiFi &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</div>
                                                    <input type="text" class="form-control" placeholder="Amount" value="@Model.MAC_WiFi">
                                                    <div class="input-group-addon"><span class="fa fa-wifi"></span> </div>
                                                </div>

                                                <div class="input-group">
                                                    <div class="input-group-addon">CPU &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</div>
                                                    <input type="text" class="form-control" placeholder="Amount" value="@Model.CPU">
                                                    <div class="input-group-addon"><span class="fa fa-tasks"></span> </div>
                                                </div>

                                                <div class="input-group">
                                                    <div class="input-group-addon">RAM &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</div>
                                                    <input type="text" class="form-control" placeholder="Amount" value="@Model.RAM">
                                                    <div class="input-group-addon"><span class="fa fa-exclamation"></span> </div>
                                                </div>

                                                <div class="input-group">
                                                    <div class="input-group-addon">STORAGE &nbsp;&nbsp;&nbsp;&nbsp;</div>
                                                    <input type="text" class="form-control" placeholder="Amount" value="@Model.STORAGE">
                                                    <div class="input-group-addon"><span class="fa fa-hdd-o"></span> </div>
                                                </div>

                                                <div class="input-group">
                                                    <div class="input-group-addon">Pantalla</div>
                                                    <input type="text" class="form-control" placeholder="Amount" value="@Model.Pantalla">
                                                    <div class="input-group-addon"><span class="fa fa-desktop"></span> </div>
                                                </div>
                                            </div>                                    
                                        </div>
                                    </center>
                                </div>
                            </div>
                        </div>
                        <div class="panel">
                            <a class="panel-heading collapsed" role="tab" id="headingThree" data-toggle="collapse" data-parent="#accordion" href="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
                                <h4 class="panel-title">Otra Informacion</h4>
                            </a>
                            <div id="collapseThree" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
                                <div class="panel-body">
                                    <center>
                                        <div class="form-inline">
                                            <div class="form-group">
                                                <div class="input-group">
                                                    <div class="input-group-addon">Status</div>
                                                    <input type="text" class="form-control" placeholder="Amount" value="@Model.sis_estatusequipo.Descripcion">
                                                    <div class="input-group-addon"><span class="fa fa-flag-checkered"></span> </div>
                                                </div>

                                                <div class="input-group">
                                                    <div class="input-group-addon">Mantenimiento</div>
                                                    <input type="text" class="form-control" placeholder="Amount" value="@Model.sis_mantenimiento.Descripcion">
                                                    <div class="input-group-addon"><span class="fa fa-users"></span> </div>
                                                </div>

                                                <div class="input-group">
                                                    <div class="input-group-addon">Marca </div>
                                                    <input type="text" class="form-control" placeholder="Amount" value="@Model.sis_marcas.Descripcion">
                                                    <div class="input-group-addon"><span class="fa fa-copyright"></span> </div>
                                                </div>

                                                <div class="input-group">
                                                    <div class="input-group-addon">Tipo Equipo &nbsp;&nbsp;&nbsp;</div>
                                                    <input type="text" class="form-control" placeholder="Amount" value="@Model.sis_tipoequipos.Descripcion">
                                                    <div class="input-group-addon"><span class="fa fa-laptop"></span> </div>
                                                </div>
                                            </div>
                                        </div>
                                    </center>                                                               
                                </div>                                
                            </div>                            
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
    </div>
</div>